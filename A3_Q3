public class FrequencyofElementinArray {
    public static void main(String[] args) 
    {
        int[] arr = {3,24,12,65,54,12,33,12};
        System.out.println("Frequency of x in Array is: "+findFrequency(arr,12));
    }

    static int findFrequency(int[] arr, int x) 
    {
        int f = 0;
        for (int i = 0; i < arr.length; i++) {
            if (arr[i] == x) {
                f++;
            }
        }
        return f;
    }
}
